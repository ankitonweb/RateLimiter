{
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-flow"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "debug": "~4.1.1",
    "express": "^4.17.1",
    "hot-shots": "^7.7.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.1",
    "redis": "^3.1.2",
    "segfault-handler": "^1.3.0"
  },
  "main": "src/RateLimiter.js",
  "name": "RateLimiter",
  "nodemonConfig": {
    "exec": "npm run kill && npm run dev",
    "watch": [
      "src"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ],
    "delay": 300
  },
  "private": true,
  "scripts": {
    "lint": "npm run eslint .",
    "start": "npm run clean && npm run transpile && node ./dist-example/index.js ",
    "build": "npm run clean && npm run transpile",
    "server": "node ./dist-example/index.js ",
    "flow": "flow",
    "format": "prettier --write ./src",
    "transpile": "babel ./src  --out-dir dist && babel ./examples --out-dir dist-example ",
    "clean": "rm -rf dist-example/ dist/",
    "watch:dev": "nodemon --legacy-watch",
    "test": "mocha --require @babel/register --exit",
    "kill": "kill $(lsof -t -i:9071) | exit 0"
  },
  "version": "0.0.1",
  "author": {
    "name": "Ankit Bhandari",
    "email": "ankitonweb@gmail.com"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-flow": "^7.10.1",
    "@babel/register": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "batch-api-requests": "0.0.1",
    "chai": "^4.1.0",
    "eslint": "^7.2.0",
    "eslint-plugin-flowtype": "^5.1.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.4.0",
    "flow-bin": "^0.126.1",
    "mocha": "^9.1.0",
    "mock-require": "^3.0.3",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.1.0",
    "supertest": "^3.0.0"
  }
}
